<hi>All Tags</h1><br />

  <ul id="tags">
    <% @tags.each do |tag| %>
      <li>
        <%= link_to tag.name, tag_path(tag) %>
        <%= link_to "delete", tag_path(tag), method: :delete, data: {confirm: "Really delete the tag?"} %>
      </li><br />
    <% end %><br />
  </ul><br />
  <%= link_to "<< Back to Articles List", articles_path %>
